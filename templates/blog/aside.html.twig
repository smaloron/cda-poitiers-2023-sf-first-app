


    <h4>Liste des auteurs</h4>
    <ul class="list-group">
        {% for item in countByAuthor %}
            <li class="list-group-item">
                <a href="{{ path('blog_by_author', {'id': item.author_id}) }}">
                {{ item.nickName }}&nbsp;({{ item.nb }})
                </a>
            </li>
        {% endfor %}
    </ul>

    <h4 class="mt-3">Liste des tags</h4>
    <div>
        {% for item in countByTag %}
            <a href="{{ path('blog_by_tag', {'id': item.id}) }}">
                {{ item.tagName }}&nbsp;({{ item.nb }})
            </a>
        {% endfor %}
    </div>

    <h4 class="mt-3">Liste des Ann√©es</h4>
    <div>
        {% for item in countByYear %}
            <a href="{{ path('blog_by_year', {'year': item.yearPublished}) }}">
                {{ item.yearPublished }}&nbsp;({{ item.nb }})
            </a>
        {% endfor %}
    </div>



